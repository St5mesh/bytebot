# KoboldCpp Configuration
# Set the base URL for your KoboldCpp instance running on your home network
# Replace the IP address with your actual KoboldCpp server IP and port
KOBOLDCPP_BASE_URL=http://192.168.1.100:5001

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/bytebotdb

# Desktop Service Configuration
BYTEBOT_DESKTOP_BASE_URL=http://bytebot-desktop:9990
BYTEBOT_AGENT_BASE_URL=http://bytebot-agent:9991
BYTEBOT_DESKTOP_VNC_URL=http://bytebot-desktop:9990/websockify
BYTEBOT_LLM_PROXY_URL=http://bytebot-llm-proxy:4000

# Optional: Keep these empty if using only KoboldCpp
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=